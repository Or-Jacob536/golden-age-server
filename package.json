{
    "name": "golden-age-server",
    "version": "1.0.0",
    "description": "Backend server for Golden Age elderly care facility app",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "test": "jest",
      "migrate": "sequelize-cli db:migrate",
      "seed": "sequelize-cli db:seed:all",
      "reset-db": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all"
    },
    "dependencies": {
      "axios": "^1.4.0",
      "bcryptjs": "^2.4.3",
      "cookie-parser": "^1.4.6",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "express-rate-limit": "^6.7.0",
      "helmet": "^7.0.0",
      "jsonwebtoken": "^9.0.0",
      "morgan": "^1.10.0",
      "nodemailer": "^6.9.3",
      "pg": "^8.11.0",
      "pg-hstore": "^2.3.4",
      "sequelize": "^6.32.0",
      "ws": "^8.13.0",
      "xml2js": "^0.6.0"
    },
    "devDependencies": {
      "jest": "^29.5.0",
      "nodemon": "^2.0.22",
      "sequelize-cli": "^6.6.1",
      "supertest": "^6.3.3"
    },
    "engines": {
      "node": ">=16.0.0"
    }
